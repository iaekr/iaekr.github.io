# Server
server <- function(input, output, session) {
output$slider <- renderUI({
if (input$variable %in% c("Company", "Cpu", "Ram", "TypeName")) {
return(NULL)  # No slider for categorical variables
} else {
return(sliderInput("numInputs", paste("Number of unique", input$variable, "to plot:"),
min = 1, max = length(unique(filtered_data[[input$variable]])), value = 1))
}
})
output$plot <- renderPlot({
if (input$variable %in% c("Company", "Cpu", "Ram", "TypeName")) {
ggplot(filtered_data, aes_string(x = input$variable, y = "Price", color = input$variable)) +
geom_point(size = 3) +
labs(x = input$variable, y = "Price (SGD)", color = input$variable)
} else {
top_inputs <- head(unique(filtered_data[[input$variable]]), input$numInputs)
ggplot(filtered_data %>% filter({{input$variable}} %in% top_inputs),
aes_string(x = input$variable, y = "Price", color = input$variable)) +
geom_point(size = 3) +
labs(x = input$variable, y = "Price (SGD)", color = input$variable)
}
})
}
